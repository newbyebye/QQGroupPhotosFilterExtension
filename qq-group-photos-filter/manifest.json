{
    "manifest_version": 2,

    "name": "QQ群筛选图片插件",
    "description": "先进入某个相册，然后输入QQ号筛选图片",
    "version": "1.0.1",

    "permissions": [
        "http://*.qzone.qq.com/",
        "webRequestBlocking",
        "webRequest"
    ],

    "browser_action": {
        "default_title": "筛选QQ群图片",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "icons": {
        "96": "icon.png"
    },

    "background": {
        "page": "background.html"
    }
}
